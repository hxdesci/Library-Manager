{
  "rules": {
    ".read": true,  
    ".write": "auth != null",
    "books": {
      ".indexOn": ["title", "author"],
      ".read": true,
      ".write": "auth != null"
    },
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    }
  }
}
